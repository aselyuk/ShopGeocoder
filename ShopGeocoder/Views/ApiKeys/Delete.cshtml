@model ShopGeocoder.Models.ApiKey

@{
    ViewData["Title"] = "Удаление API ключа";
}

    <div class="col-8 offset-2">
        <h1>Удаление API ключа</h1>

        <h3>Вы действительно хотите удалить API ключ?</h3>
        <div>
            <h4>ApiKey</h4>
            <hr />
            <dl class="row">
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Email)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Email)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Key)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Key)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Description)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Description)
                </dd>
                <dt class="col-sm-2">
                    @Html.DisplayNameFor(model => model.Default)
                </dt>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.Default)
                </dd>
            </dl>

            <form asp-action="Delete">
                <input type="hidden" asp-for="Guid" />
                <input type="submit" value="Удалить" class="btn btn-danger" /> |
                <a class="btn btn-sm btn-light" asp-action="Index">Вернуться к списку</a>
            </form>
        </div>
    </div>